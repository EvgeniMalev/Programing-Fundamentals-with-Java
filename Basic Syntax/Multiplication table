import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        int a = scanner.nextInt();

        if (a < 1 || a > 100) {
            return;
        }

        for (int i = 1; i <= 10; i++) {
            int b = a * i;
            System.out.printf("%d X %d = %d\n", a, i, b);
        }

        scanner.close(); // close the scanner when done
    }
}
