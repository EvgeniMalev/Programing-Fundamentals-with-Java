   int r = Integer.parseInt(scanner.nextLine());
   
  int rectanglearea = a*b;
  int squarearea = d*d;
  int circlearea = 2*3.14*r;
  p = m+n+o; p1 = p/2;
  int trianglearea = peqrt(p1*(p1-a)*(p1-b)*(p1-c));
    
  
  
