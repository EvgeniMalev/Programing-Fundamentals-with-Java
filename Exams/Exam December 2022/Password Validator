import java.util.regex.Matcher;
import java.util.regex.Pattern;
import java.util.Scanner;

public class Registration {

    public static void main(String[] args) {
      
        Scanner scan = new Scanner(System.in);
        Pattern patt = Pattern.compile("(U$)(?<username>[A-Z]{1}[a-z]{2,})\1(P@$)(?<password>[A-Za-z]{4,}[0-9]{1,})\3$");
        int n = Integer.parseInt(scan.nextLine());
        int validRegistration = 0;
      
         for (int i = 0; i < n; i++)
         {
         String input = scan.nextLine();
           
            Matcher mtch = patt.matcher(input);
           
           if (mtch.find()){
           System.out.println("Registration was successful %n");
           System.out.printf("Username: %s, Password: %s", mtch.group("username"), mtch.group("password"));
           validRegistration++; 
           }
               else
               {
           System.out.println("Invalid username or password");  
               }
         }
        System.out.printf("Successful registrations: %d", validRegistration);
    }
} 
